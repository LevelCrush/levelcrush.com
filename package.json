{
  "name": "levelcrush",
  "version": "0.0.0",
  "description": "LevelCrushWebsite",
  "main": "dist/app.js",
  "author": {
    "name": ""
  },
  "scripts": {
    "build": "npm run build:css && tsc --build",
    "clean": "tsc --build --clean",
    "debug": "npm run build:css && ts-node ./src/app.ts",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "production": "npm run build:css && node ./dist/app.js",
    "build:css": "postcss assets/css/tailwind-build.css -o assets/css/tailwind.css",
    "watch:css" : "postcss assets/css/tailwind-build.css -o assets/css/tailwind.css --watch"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/express-session": "^1.17.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^14.14.7",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.23",
    "ts-node": "^10.4.0",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@types/pug": "^2.0.6",
    "autoprefixer": "^10.4.2",
    "axios": "^0.26.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.1",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "express-session": "^1.17.2",
    "moment": "^2.29.1",
    "multer": "^1.4.4",
    "postcss": "^8.4.8",
    "postcss-cli": "^9.1.0",
    "pug": "^3.0.2"
  }
}
